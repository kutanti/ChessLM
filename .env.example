# Copy this file to .env.local and fill in your API keys

# Azure OpenAI (recommended)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://eastus-demo-oai.openai.azure.com/
AZURE_OPENAI_API_VERSION=2025-06-01

# OpenAI API Key (alternative to Azure OpenAI)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Replicate API Token (for Llama and other models)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Next.js URL (for production)
NEXTAUTH_URL=http://localhost:3000

# Database URL (if using a database for game history)
DATABASE_URL=your_database_url_here
